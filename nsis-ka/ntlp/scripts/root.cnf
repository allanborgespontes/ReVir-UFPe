[ req ]
default_bits = 1024
encrypt_key = yes
distinguished_name = req_dn
x509_extensions = v3_ca
prompt = no

[ req_dn ]
C=DE
O=TM-UKA
CN=NSIS Root CA TM.UKA.DE

[ cert_type ]
nsCertType = server

[ v3_ca ]
subjectKeyIdentifier=hash
authorityKeyIdentifier=keyid:always,issuer:always
basicConstraints = CA:true

